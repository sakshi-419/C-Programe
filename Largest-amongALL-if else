#include <stdio.h>
int main(){
    int num1 =15,num2= 10, num3 = 5;
    printf("Enter the number\n");
    scanf("%d %d %d",&num1 , &num2 , &num3);
    if(num1 >= num2 && num1>= num3){
        printf(" %d is largest number among all\n", num1);
    }
    else if(num2 >= num1 && num2 >=num3){
         printf("%d   is largest number among all\n",num2);
    }
    else{
        printf("%d   is largest nnumber among all",num3);
    }
    return 0;

}
